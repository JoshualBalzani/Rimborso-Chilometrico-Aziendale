# Configurazione Rimborso KM
# Copia in .env e compila i valori

# Flask Environment
FLASK_ENV=development
FLASK_DEBUG=True

# Google Maps API Key (opzionale)
# Genera da: https://console.cloud.google.com
# Abilita Distance Matrix API
GOOGLE_MAPS_API_KEY=

# Database (SQLite - non modificare se locale)
DATABASE_URL=sqlite:///data/app.db

# Security (per produzione)
SECRET_KEY=your-secret-key-change-in-production
